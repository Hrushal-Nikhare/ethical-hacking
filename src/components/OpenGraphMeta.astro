---
export interface OpenGraphMetaProps {
  title: string;
  description: string;
  url: string;
  image: string;
}
---
<head>
  <meta property="og:title" content={Astro.props.title} />
  <meta property="og:description" content={Astro.props.description} />
  <meta property="og:url" content={Astro.props.url} />
  <meta property="og:image" content={Astro.props.image} />
</head>